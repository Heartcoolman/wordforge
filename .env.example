# Runtime
HOST=127.0.0.1
PORT=3000
RUST_LOG=info

# Storage
SLED_PATH=./data/learning.sled

# Auth
JWT_SECRET=change_me_to_random_64_chars
JWT_EXPIRES_IN_HOURS=24
ADMIN_JWT_SECRET=change_me_to_another_random_64_chars

# Network
CORS_ORIGIN=http://localhost:5173
TRUST_PROXY=false

# Rate limit
RATE_LIMIT_WINDOW_SECS=900
RATE_LIMIT_MAX=500

# Worker
WORKER_LEADER=true
ENABLE_LLM_ADVISOR_WORKER=false
ENABLE_ENGINE_MONITORING_WORKER=true

# AMAS
AMAS_ENSEMBLE_ENABLED=true
AMAS_MONITOR_SAMPLE_RATE=0.05

# External Services (optional)
LLM_ENABLED=false
LLM_MOCK=true
LLM_API_URL=
LLM_API_KEY=
LLM_TIMEOUT_SECS=30
